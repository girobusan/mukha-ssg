{
  "name": "mukha-ssg",
  "version": "0.1.2b",
  "description": "Minimalist static site generator",
  "keywords": [
    "ssg",
    "static",
    "static-site-generator"
  ],
  "bin": {
    "mukha-ssg": "dist/mukha.js",
    "latid2mukha": "dist/latid2mukha.js"
  },
  "files": [
    "dist/mukha.js",
    "dist/oss-licenses.json",
    "dist/search_client.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --mode production",
    "watch": "npx webpack --mode development",
    "prebuild": "npx webpack -c prebuild.config.js --mode production",
    "postbuild": "shx chmod +x dist/mukha.js && shx chmod +x dist/latid2mukha.js"
  },
  "babel": {
    "plugins": [
      [
        "htm"
      ]
    ]
  },
  "author": "",
  "license": "LGPL",
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "babel-plugin-htm": "^3.0.0",
    "css-loader": "^6.7.1",
    "htm": "^3.1.1",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-license-plugin": "^4.5.1",
    "webpack-shell-plugin-next": "^2.3.2",
    "date-fns": "^4.1.0",
    "feed": "^5.1.0",
    "highlight.js": "^11.11.1",
    "install": "^0.13.0",
    "js-yaml": "^4.1.0",
    "lodash.throttle": "^4.1.1",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.14.0",
    "markdown-it": "^13.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-multimd-table": "^4.1.3",
    "markdown-it-small": "^1.0.0",
    "nunjucks": "^3.2.4",
    "open": "^10.1.2",
    "papaparse": "^5.5.3",
    "path-browserify": "^1.0.1",
    "unidecode": "^1.1.0",
    "yoctocolors": "^2.1.1"
  },
  "dependencies": {}
}
